{
    "editor.tabSize": 2,
    "files.exclude": {
        "**/.git": true
    },
    "search.exclude": {
        "**/coverage": true,
        "**/node_modules": true
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    }
}
